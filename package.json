{
  "name": "vuwi-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "install:vuwi": "rm pnpm-lock.yaml && rm -rf node_modules && pnpm i",
    "theme:copy": "cp -rf ./node_modules/vuwi/dist/theme theme"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "@vueuse/core": "^7.5.3",
    "maska": "^1.5.0",
    "vee-validate": "^4.5.5",
    "vue": "^3.2.25",
    "vue-demi": "latest",
    "vuwi": "file:~/local_modules/vuwi.tgz"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-windicss": "^1.6.2",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.4.2"
  }
}
